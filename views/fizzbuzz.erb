
<!--     <% numbers = (1..@number) %> -->
    <% @numbers.each do |num| %>
        <ul>
            <% if num % 3 == 0 && num % 5 == 0 %>
                <%= "FizzBuzz" %>
            <% elsif num % 3 == 0 %>
                <%= "Fizz" %>
            <% elsif num % 5 == 0 %>
                <%= "Buzz" %>
            <% else %>
                <%= num %>
            <% end %>
        </ul>
    <% end %>
 
